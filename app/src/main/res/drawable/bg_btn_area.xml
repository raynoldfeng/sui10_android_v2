<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/dp_3"/>
            <stroke android:color="@color/common_bg_divider" android:width="@dimen/dp_1"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/common_bg_dark"/>
            <corners android:radius="@dimen/dp_3"/>
            <stroke android:color="@color/common_bg_dark" android:width="@dimen/dp_1"/>
        </shape>
    </item>
</selector>